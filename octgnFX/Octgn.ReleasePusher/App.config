<?xml version="1.0"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="Octgn.ReleasePusher.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="Octgn.ReleasePusher.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <userSettings>
        <Octgn.ReleasePusher.Settings>
            <setting name="ServerPath" serializeAs="String">
                <value>C:\server\OCTGNServerFiles\</value>
            </setting>
            <setting name="CurrentVersionTemplate" serializeAs="String">
                <value>&lt;info&gt;
  &lt;release&gt;
    &lt;version&gt;{version}&lt;/version&gt;
    &lt;installPath&gt;{installPath}&lt;/installPath&gt;
    &lt;updatePath&gt;{updatePath}&lt;/updatePath&gt;
  &lt;/release&gt;
&lt;/info&gt;</value>
            </setting>
        </Octgn.ReleasePusher.Settings>
    </userSettings>
    <applicationSettings>
        <Octgn.ReleasePusher.Settings>
            <setting name="OldVersionFile" serializeAs="String">
                <value>c:\server\OctgnCurrentVersion.txt</value>
            </setting>
        </Octgn.ReleasePusher.Settings>
    </applicationSettings>

    <root>
      <!-- Levels: 1=OFF,2=FATAL,3=ERROR,4=WARN,5=INFO,6=DEBUG,7ALL -->
      <level value="ALL"/>
      <appender-ref ref="FileAppender"/>
      <appender-ref ref="ConsoleAppender" />
    </root>
  <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%date [%thread] %level %logger [%ndc] - %message%newline %exception"/>
    </layout>
  </appender>
</configuration>
